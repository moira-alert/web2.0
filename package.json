{
    "name": "moira-front",
    "version": "1.1.0",
    "license": "MIT",
    "scripts": {
        "serve": "webpack-dev-server --mode=development",
        "start-with-local-api": "cross-env API_MODE=local yarn serve",
        "build": "webpack --mode=production",
        "storybook": "cross-env NODE_ENV=development start-storybook -p 9001 -c .storybook --ci",
        "fakeapi": "json-server --watch fakeapi/db.json --routes fakeapi/routes.json --port 9002",
        "start": "npm-run-all --parallel serve fakeapi",
        "lint": "eslint --ext .tsx --ext .ts src/",
        "test": "cross-env NODE_ENV=testing jest \"./src\"",
        "test:e2e": "cross-env NODE_ENV=testing TYPE=e2e jest ./src/tests --runInBand",
        "prettier": "prettier \"./src/**/*.{ts,tsx}\"",
        "creevey": "wait-on http://localhost:9001 -t 60000 && node ./.creevey/start.js  -c ./.creevey/creevey.config.js",
        "creevey:ui": "yarn creevey --ui"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint && yarn prettier && npx tsc --noEmit"
        }
    },
    "dependencies": {
        "@skbkontur/react-icons": "4.1.0",
        "@skbkontur/react-stack-layout": "1.0.3",
        "@skbkontur/react-ui": "2.4.0",
        "@skbkontur/react-ui-validations": "1.1.3",
        "bundle-loader": "0.5.6",
        "color-hash": "1.0.3",
        "date-fns": "2.9.0",
        "dompurify": "^1.0.10",
        "file-saver": "2.0.2",
        "husky": "4.2.3",
        "jest": "24.5.0",
        "lodash": "4.17.13",
        "npm-run-all": "4.1.5",
        "numeral": "2.0.6",
        "query-string": "6.4.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-foco": "1.2.0",
        "react-hot-loader": "4.8.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "remarkable": "1.7.1"
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.9.5",
        "@babel/plugin-transform-destructuring": "7.9.5",
        "@babel/polyfill": "7.2.5",
        "@babel/preset-env": "7.9.5",
        "@babel/preset-flow": "7.9.0",
        "@babel/preset-react": "7.9.4",
        "@babel/preset-typescript": "7.10.4",
        "@babel/register": "7.9.0",
        "@hot-loader/react-dom": "16.13.0",
        "@storybook/addon-actions": "^5.3.17",
        "@storybook/addons": "^5.3.17",
        "@storybook/react": "^5.3.17",
        "@types/classnames": "2.2.10",
        "@types/jest": "26.0.9",
        "@types/jest-environment-puppeteer": "4.3.2",
        "@types/puppeteer": "^3.0.1",
        "@types/react": "16.9.43",
        "@types/react-dom": "16.9.8",
        "@types/react-router-dom": "5.1.5",
        "@typescript-eslint/eslint-plugin": "3.7.0",
        "@typescript-eslint/parser": "3.7.0",
        "aws-sdk": "^2.649.0",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.0.5",
