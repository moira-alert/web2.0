{
    "name": "moira-front",
    "version": "1.1.0",
    "license": "MIT",
    "scripts": {
        "serve": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
        "start-with-local-api": "cross-env API_MODE=local yarn serve",
        "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
        "storybook": "cross-env NODE_ENV=development start-storybook -p 9001 -c .storybook --ci",
        "fakeapi": "json-server --watch fakeapi/db.json --routes fakeapi/routes.json --port 9002",
        "start": "npm-run-all --parallel serve fakeapi",
        "lint": "eslint --ext .jsx --ext .js src/",
        "test": "cross-env NODE_ENV=testing jest \"./src\"",
        "prettier": "prettier \"./src/**/*.js\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint && yarn prettier"
        }
    },
    "dependencies": {
        "@skbkontur/react-icons": "3.1.3",
        "@skbkontur/react-stack-layout": "^1.0.3",
        "bundle-loader": "0.5.6",
        "color-hash": "1.0.3",
        "date-fns": "2.9.0",
        "dompurify": "^1.0.10",
        "husky": "4.2.3",
        "jest": "24.5.0",
        "lodash": "4.17.13",
        "npm-run-all": "4.1.5",
        "numeral": "2.0.6",
        "query-string": "6.4.0",
        "react": "16.8.4",
        "react-dom": "16.8.4",
        "react-foco": "1.2.0",
        "react-hot-loader": "4.8.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-ui-validations": "0.3.0",
        "remarkable": "1.7.1",
        "retail-ui": "0.44.1"
    },
    "devDependencies": {
        "@babel/core": "7.3.4",
        "@babel/plugin-proposal-class-properties": "7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "7.3.4",
        "@babel/plugin-transform-destructuring": "7.3.2",
        "@babel/polyfill": "7.2.5",
        "@babel/preset-env": "7.3.4",
        "@babel/preset-flow": "7.0.0",
        "@babel/preset-react": "7.0.0",
        "@hot-loader/react-dom": "16.8.4",
        "@storybook/addon-actions": "4.1.6",
        "@storybook/addons": "4.1.6",
        "@storybook/react": "4.1.6",
        "babel-eslint": "10.0.1",
        "babel-loader": "8.0.5",
        "classnames": "2.2.6",
        "classnames-loader": "2.0.0",
        "cross-env": "5.2.0",
        "css-loader": "2.1.1",
        "eslint": "5.15.3",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-prettier": "4.1.0",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-flowtype": "3.4.2",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-prettier": "3.0.1",
        "eslint-plugin-react": "7.12.4",
        "eslint-plugin-react-hooks": "^2.5.0",
        "file-loader": "3.0.1",
        "flow-bin": "0.95.1",
        "flow-coverage-report": "0.6.1",
        "html-webpack-plugin": "3.2.0",
        "json-server": "0.14.2",
        "less": "3.9.0",
        "less-loader": "4.1.0",
        "mini-css-extract-plugin": "0.5.0",
        "prettier": "1.16.4",
        "storybook-react-router": "1.0.3",
        "style-loader": "0.23.1",
        "webpack": "4.29.6",
        "webpack-bundle-analyzer": "3.3.2",
        "webpack-cli": "3.3.0",
        "webpack-dev-server": "3.2.1",
        "webpack-merge": "4.2.1"
    }
}
