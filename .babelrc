{
    "plugins": [
        "react-hot-loader/babel",
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        ["@babel/plugin-transform-destructuring", { "loose": true }],
        ["@babel/plugin-proposal-object-rest-spread", { "loose": true }]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/typescript",
        "@babel/preset-react"
    ],
    "env": {
        "testing": {
            "presets":[
                "@babel/preset-env",
                "@babel/preset-react"
            ]
        }
    }
}
